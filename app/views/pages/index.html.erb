<h1>Albums</h1>
<ul id="albums"></ul>

<script data-name="albums" type="text/x-handlebars">
  {{#each albums}}
  <li>
    <a href="{{tracks_url}}">
      <img src="{{cover}}" />
      <h2>{{title}}</h2>
      <h3>{{artist}}</h3>
      <p>Released: {{date}}</p>
    </a>
  </li>
  {{/each}}
</script>
